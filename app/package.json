{
  "name": "data-explorer",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "node --max_old_space_size=8192 ./node_modules/.bin/svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest src/tests",
    "deploy": "gh-pages -d build -t true"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-dsv": "^2.0.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.7",
    "@sveltejs/adapter-static": "^1.0.0-next.25",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "cssnano": "^5.0.15",
    "daisyui": "^1.22.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.4",
    "leaflet": "^1.7.1",
    "leaflet-easybutton": "^2.4.0",
    "leaflet.markercluster": "^1.5.3",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup-plugin-geojson": "^0.1.0",
    "rollup-plugin-string": "^3.0.0",
    "simple-svelte-autocomplete": "^2.2.4",
    "svelte": "^3.42.6",
    "svelte-leafletjs": "github:wykhuh/svelte-leaflet",
    "svelte-markdown": "^0.2.2",
    "svelte-preprocess": "^4.10.1",
    "svelte-vega": "^1.1.0",
    "tailwindcss": "^3.0.13",
    "tippy.js": "^6.3.7",
    "vega": "^5.21.0",
    "vega-embed": "^6.20.5",
    "vega-lite": "^5.2.0"
  },
  "type": "module",
  "dependencies": {
    "lodash.groupby": "^4.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ]
  }
}
